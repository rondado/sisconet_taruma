  object QryOrdServ: TSQLQuery
    MaxBlobSize = 1
    Params = <
      item
        DataType = ftDate
        Name = 'pdatai'
        ParamType = ptInput
      end
      item
        DataType = ftDate
        Name = 'pdataf'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'pStatus'
        ParamType = ptInput
      end>
    SQL.Strings = (
      'select * from ord_servico s'
      'where s.data_os between :pdatai and :pdataf'
